<?xml version="1.0"?>

<system name="all">
	<description>
		description: "XAce file for Goanna Log4e compile all test"
		project: "Goanna [http://sourceforge.net/projects/goanna]"
		library: "log4e"
		author: "Glenn Maughan [mailto:glennmaughan@goanna.info]"
		copyright: "Copyright (c) 2002, Glenn Maughan"
		license: "Eiffel Freeware License v1 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<root class="LOG4E_ALL" creation="make"/>
	
	<option name="multithreaded" value="true"/>

	<cluster name="root_cluster" location="${LOG4E}/test/all"/>
	
	<mount location="${GOBO}/library/library.xace"/>
	<mount location="${GOBO}/library/kernel.xace"/>
	<mount location="${LOG4E}/library/library.xace"/>
	<mount location="${LOG4E}/library/other/eiffelnet.xace"/>
	<mount location="${LOG4E}/library/other/eiffelthread.xace"/>

</system>

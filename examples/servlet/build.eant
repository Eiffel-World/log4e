<?xml version="1.0"?>

<project name="log4e_servlet" default="help">

	<description>
		description: "Log4e configuration servlet example"
		project: "Goanna [http://sourceforge.net/projects/goanna]"
		library: "log4e"
		author: "Glenn Maughan [mailto:glennmaughan@goanna.info]"
		copyright: "Copyright (c) 2002, Glenn Maughan"
		license: "Eiffel Freeware License v1 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${LOG4E}/misc/eiffel_ise_only.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="log4e_servlet"/>
		<set name="system_dir" value="${LOG4E}/examples/servlet"/>
	</target>

</project>
